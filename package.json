{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "Isomorfic starter kit for react apps",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/server.js",
    "clean": "if test -d ./build; then rm -r build; fi",
    "clean:stats": "if test -f build/stats.json; then rm build/stats.json; fi",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:server": "./node_modules/webpack/bin/webpack.js --config config/webpack/env/production.js --bail",
    "build:client": "./node_modules/webpack/bin/webpack.js --config config/webpack/env/build.js --bail -p",
    "dev": "npm run clean:stats && npm run dev:server & npm run dev:client",
    "dev:client": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config config/webpack/env/development.js --hot --host '0.0.0.0' --port 8080",
    "dev:server": "npm run dev:server:webpack & npm run dev:server:nodemon",
    "dev:server:webpack": "./node_modules/webpack/bin/webpack.js --config config/webpack/env/server.js --watch",
    "dev:server:nodemon": "DEBUG=square ./node_modules/nodemon/bin/nodemon.js -e js,json -w ./build/ ./build/server.js",
    "lint": "./node_modules/.bin/eslint ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2gis/isomorphic-react-starter-kit.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.6.4",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "2.2.x",
    "eslint-plugin-react": "^4.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "nodemon": "^1.9.1",
    "null-loader": "^0.1.1",
    "postcss-assets": "^4.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.1",
    "postcss-mixins": "^4.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.3",
    "postcss-simple-vars": "^1.2.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
